{
    "name": "pieterxjan/slack-error-notifications",
    "description": "A Craft CMS plugin used to log error in your Craft website to a channel on Slack",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "slack",
        "error-handling",
        "notificaions"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pieter-Jan Claeysens",
            "homepage": "https://github.com/pieterxjan"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "pieterxjan\\slackerrornotifications\\": "src/"
        }
    },
    "extra": {
        "name": "Slack error notifications",
        "handle": "slack-error-notifications",
        "class": "pieterxjan\\slackerrornotifications\\SlackErrorNotifications",
        "components": {
            "slackService": "pieterxjan\\slackerrornotifications\\services\\SlackService"
        }
    }
}
